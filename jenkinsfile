pipeline {
    agent any 
    stages {
	    stage('Checkout') {
            steps {
                checkout scm
            }
        }
        stage('Example Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Example Test') {
            steps {
                echo 'Testing'
            }
        }
		stage('Example Test') {
            steps {
                bat 'python hello.py' 
            }
        }
    }
}