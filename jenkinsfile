pipeline {
    agent any 
    stages {
	    stage('Checkout') {
            steps {
                checkout scm
            }
        }
        stage('Example Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Example Test') {
            steps {
                echo 'Testing'
            }
        }
		stage('listing') {
            steps {
                bat 'dir' 
            }
        }
		stage('execute shell') {
            steps {
				script{
					def statuscode = sh script:"Test Pipeline/script/hello.sh", returnStatus:true
					echo statuscode
				}
            }
        }
    }
}